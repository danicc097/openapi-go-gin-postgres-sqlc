{
  "compilerOptions": {
    "jsxImportSource": "@emotion/react",
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "skipLibCheck": true,
    "allowJs": false,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "verbatimModuleSyntax": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "sourceMap": true,
    "paths": {
      "@roles": [
        "roles.json"
      ],
      "@scopes": [
        "scopes.json"
      ],
      "@operationAuth": [
        "operationAuth.gen.json"
      ],
      "@config": [
        "config.json"
      ],
    },
  },
}
